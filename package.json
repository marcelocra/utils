{
  "name": "mcra-utils",
  "version": "0.1.3",
  "description": "Be more productive by automating tasks and avoiding code rewrites.",
  "bin": {
    "mcra": "dist/bin.js"
  },
  "type": "module",
  "main": "dist/lib.js",
  "scripts": {
    "sc": "shadow-cljs",
    "server": "shadow-cljs stop && shadow-cljs start",
    "dev:bin": "shadow-cljs watch bin",
    "dev:lib": "shadow-cljs watch lib",
    "rel:bin": "pnpm clean && shadow-cljs release bin",
    "rel:lib": "pnpm clean && shadow-cljs release lib",
    "test": "shadow-cljs watch test --config-merge \"{:autorun true}\"",
    "test:once": "shadow-cljs compile test && node out/test.js",
    " ": "",
    "NEED TO REVIEW THE FOLLOWING ----------------------------------------": "",
    "  ": "",
    "e2e": "shadow-cljs compile e2e && node out/e2e.js",
    "quick-e2e": "shadow-cljs compile lib && yarn e2e",
    "lint": "clj-kondo --lint src",
    "format": "zprint-clj --hang -i \"./src/**/*.{clj,cljs,cljc,edn}\" -o ./ && zprint-clj -i \"./*.edn\" -o ./",
    "clean": "shx rm -rf dist/"
  },
  "author": {
    "name": "Marcelo Almeida",
    "email": "npm@marcelocra.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcelocra/utils"
  },
  "license": "MIT",
  "dependencies": {
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "shadow-cljs": "^2.28.7",
    "shx": "^0.3.4"
  }
}
